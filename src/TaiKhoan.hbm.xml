<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate
Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class catalog="QuanLySinhVien"
			name="pojo.TaiKhoan" table="TAIKHOAN">
		<id name="tenDangNhap" type="string">
			<column length="20" name="TENDANGNHAP" />
		</id>
		<property name="matKhau" type="string">
			<column length="100" name="MATKHAU" />
		</property>
		<property name="loaiTaiKhoan" type="string">
			<column length="2" name="LOAITAIKHOAN" />
		</property>
	</class>
</hibernate-mapping>