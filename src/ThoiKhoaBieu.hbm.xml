<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate
Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class catalog="QuanLySinhVien"
			name="pojo.ThoiKhoaBieu" table="THOIKHOABIEU">
		<id name="stt" type="int">
			<column name="STT" />
		</id>
		<property name="maMon" type="string">
			<column length="100" name="MAMON" />
		</property>
		<property name="tenMon" type="string">
			<column length="3" name="TENMON" />
		</property>
		<property name="phongHoc" type="string">
			<column length="20" name="PHONGHOC" />
		</property>
		<property name="lop" type="string">
			<column length="20" name="LOP" />
		</property>
	</class>
</hibernate-mapping>